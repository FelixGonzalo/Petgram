{
    "version": 2,
    "name": "petgram-felixcastro",
    "builds": [{ "use": "@vercel/static-build", "src": "package.json" }],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "(.*).png",
            "dest": "/$1.png"
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"
        },
        {
            "src": "/.*",
            "dest": "index.html"
        }
    ]
}
